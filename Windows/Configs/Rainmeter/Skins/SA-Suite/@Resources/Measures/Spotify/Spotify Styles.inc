[SpotifyIcon]
ImagePath=#@#Images\Spotify\
ImageTint=#ColorCode#
AntiAlias=1
DynamicVariables=1

[SpotifyPosition]
ImageName=#@#Images\Spotify\Position.png
ImageTint=#ColorCode#
AntiAlias=1
DynamicVariables=1
